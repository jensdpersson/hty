<testspec>
  <synopsis>
    Serve static content through the <b>static</b> resource class.
  </synopsis>
  <transaction>
    <request>
      <url>http://localhost:8080/</url>
    </request>
    <response>
      <status>200</status>
      <header name="content-type">application/xml</header>
      <body>
	       <exact file="facit.html"/>
      </body>
    </response>
  </transaction>
</testspec>
