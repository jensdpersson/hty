<testspec>
  <synopsis>
    Move a file
  </synopsis>
  <transaction>
    <request>
      <url>http://localhost:10888/oldname</url>
      <method>get</method>
    </request>
    <response>
      <status>200</status>
      <body>
	    <exact file="datafile"/>
      </body>
    </response>
  </transaction>
  
  <transaction>
    <request>
      <url>http://localhost:10888/newname</url>
      <method>get</method>
    </request>
    <response>
      <status>404</status>
    </response>
  </transaction>
  
  <transaction>
    <request>
      <url>http://localhost:10888/oldname</url>
      <method>delete</method>
      <header name="Destination">newname</header>
      <header name="Overwrite">F</header>
      <header name="X-HTTP-Method-Override">MOVE</header>
    </request>
    <response>
      <status>200</status>
      <header name="Location">newname</header>
    </response>
  </transaction>
  
  <transaction>
    <request>
      <url>http://localhost:10888/newname</url>
      <method>get</method>
    </request>
    <response>
      <status>200</status>
      <body>
	    <exact file="datafile"/>
      </body>
    </response>
  </transaction>
  
  <transaction>
    <request>
      <url>http://localhost:10888/oldname</url>
      <method>get</method>
    </request>
    <response>
      <status>404</status>
    </response>
  </transaction>
  
</testspec>
