<testspec>
  <synopsis>
    Get an index.txt by asking for its folder
  </synopsis>
  <transaction>
    <request>
      <url>http://localhost:1031/subfolderwithtextindex/</url>
    </request>
    <response>
      <status>200</status>
      <header name="content-type">text/plain</header>
      <body>
	       <exact file="facit.txt"/>
      </body>
    </response>
  </transaction>
</testspec>
