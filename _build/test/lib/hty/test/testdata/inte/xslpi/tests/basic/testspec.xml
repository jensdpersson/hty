<testspec>
  <synopsis>
    Send the xml-stylesheet (xslpi) fragment before the rest of the file.
  </synopsis>
  <transaction>
    <request>
      <url>http://localhost:10001/helloworld.xml</url>
    </request>
    <response>
      <status>200</status>
      <header name="content-type">text/xml</header>
      <body>
	       <exact file="facit.xml"/>
      </body>
    </response>
  </transaction>
</testspec>
