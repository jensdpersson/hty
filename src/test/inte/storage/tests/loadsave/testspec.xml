<testspec>
  <synopsis>
    Load and save data
  </synopsis>
  <transaction>
    <request>
      <url>http://localhost:10010/bollhav</url>
      <method>PUT</method>
      <body file="datafile"/>
    </request>
    <response>
      <status>200</status>
    </response>
  </transaction>
  <transaction>
    <request>
      <url>http://localhost:10010/bollhav</url>
    </request>
    <response>
      <status>200</status>
      <body>
	       <exact file="datafile"/>
      </body>
    </response>
  </transaction>
</testspec>
